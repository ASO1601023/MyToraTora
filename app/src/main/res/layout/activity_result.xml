<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/s_fwl"
    tools:context="com.example.miyazakikazuki.mytoratora.ResultActivity">

    <ImageView
        android:id="@+id/win"
        android:layout_width="140dp"
        android:layout_height="158dp"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/s_mig"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.524"
        app:layout_constraintRight_toLeftOf="@+id/lose"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="0.265" />

    <ImageView
        android:id="@+id/lose"
        android:layout_width="140dp"
        android:layout_height="158dp"
        app:srcCompat="@drawable/s_mig"
        android:layout_marginRight="31dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="189dp" />

    <TextView
        android:id="@+id/result_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginLeft="146dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginBottom="406dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:onClick="onBackButtonTapped"
        android:text="戻る"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
